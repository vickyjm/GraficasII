     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform vec4 lightPosition;

varying vec4 vPos;
varying vec3 vN, cLp,vC;
varying vec2 texCoord;

attribute vec3 rm_Tangent, rm_Binormal;


void main(void)
{
   vec4 lp;
  
   vN = gl_NormalMatrix * gl_Normal;
   lp = gl_ModelViewMatrix * lightPosition;
   vPos = gl_ModelViewMatrix * gl_Vertex;
   texCoord = gl_MultiTexCoord0.st;
   
   //cLp = lp.xyz;
   
   //Espacio tangente
   vec3 cam = -vPos.xyz;
   vec3 t = gl_NormalMatrix * rm_Tangent;
   vec3 b = gl_NormalMatrix * rm_Binormal;
   
   vC.x =dot(t,cam);
   vC.y = dot(b,cam);
   vC.z = dot(vN,cam);
      
   cLp.x = dot(t,lp.xyz);
   cLp.y = dot(b,lp.xyz);
   cLp.z = dot(vN,lp.xyz);
  
   gl_Position = ftransform();
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float iD,iS,iR,iA,iL,iSh,shine,snow,moho;
uniform vec4 cA,cL,cS;

varying vec4 vPos;
varying vec3 vN, cLp,vC;
varying vec2 texCoord;
uniform sampler2D Terreno_color,Terreno_normal,terreno_specular; //Texturas del terreno
uniform sampler2D terreno_snow, terreno_color_snow, terreno_snow_normal; // Texturas de la nieve
uniform sampler2D terreno_moho; // Textura para el efecto de moho
uniform sampler2D terreno_shadow; // Textura para el efecto de las sombras

uniform samplerCube txReflection;

void main(void)
{  
   vec4 colorDifuso,colorFinal, cSpec;
   float dotLN,dotRC;
   vec3 r;
   
   vec4 cColorT = texture2D(Terreno_color,texCoord);
   vec3 cNormalT = (texture2D(Terreno_normal,texCoord).xyz *2.0)*1.0;
   //vec4 cSpecularT = texture2D(terreno_specular, texCoord);
   vec4 colorSnow = texture2D(terreno_color_snow,texCoord);
   vec4 maskSnow = texture2D(terreno_snow,texCoord);
   vec3 N;
   
   N = normalize(cNormalT);
   
   //Color Difuso
   colorDifuso = mix(cColorT,cA,0);
   
   //Componente difuso
   dotLN = max(dot(normalize(cLp), normalize(N)),0.0) * iD ;
   
   //Componente Especular
   cSpec = vec4(0.0,0.0,0.0,0.0);
   r = reflect(-normalize(cLp),N);
   dotRC = pow (max(dot(normalize(r),normalize(vC)),0.0),shine) * iS;
   cSpec = cS*dotRC;
   //cSpec = cSpec*cSpecularT;
   
   //Nieve
   vec4 nieve = mix(cColorT,colorSnow*snow,maskSnow);
   
   colorFinal = iA*cA*colorDifuso*nieve + iL*(cL *(colorDifuso* dotLN + cSpec));
  
   gl_FragColor = colorFinal;
}

